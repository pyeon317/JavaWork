package quiz01; //현제 클래스는 백준 사이트의 문제를 풀기 위한 스크립트로 노션에 푼 문제들을 저장함

import java.io.*;
import java.util.Arrays;
import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);//n값이 작아서 스캐너 사용
		int N = scan.nextInt();
		int K = scan.nextInt();
		int[] arr = new int[N];
		int money = arr.length-1;//배열이 0부터 시작하니까 -1해야됨
		int cnt = 0; //코인 수
		for(int i = 0; i < arr.length; i ++) { //배열 입력
			arr[i] = scan.nextInt();
		}
		
		while(K > 0) {//K가 0이 될 때까지 반복
			if(K >= arr[money]) {
				
			}
		}
		System.out.println(cnt);
		System.out.println(K);
	}
}