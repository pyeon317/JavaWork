package quiz01; //현제 클래스는 백준 사이트의 문제를 풀기 위한 스크립트로 노션에 푼 문제들을 저장함

import java.io.*;
import java.util.Arrays;
import java.util.Scanner;
import java.util.StringTokenizer;

public class Main {
	
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);//n값이 작아서 스캐너 사용
		int N = scan.nextInt();
		int K = scan.nextInt();
		int[] arr = new int[N];
		for(int i = 0; i < arr.length; i ++) {
			arr[i] = scan.nextInt();
		}
		int cnt = 0;
		while( K != 0 ) {//K가 0이 될 때까지 반복
			if(K < 5) {//1
				cnt += K/arr[0];
				
			}else if(K < 10) {//5
				
			}else if(K < 50) {//10
				
			}else if(K < 100) {//50
				
			}else if(K < 500) {//100
				
			}else if(K < 1000) {//500
				
			}else if(K < 5000) {//1000
				
			}else if(K < 10000) {//5000
				
			}else if(K < 50000) {//10000
				
			}else {//50000
				
			}
		}
		System.out.println(cnt);
		System.out.println(K);
	}
}